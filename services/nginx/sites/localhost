server {
    listen 80;
    server_name _;
    root /home/www-data;
    autoindex on;

    location ~ \.php {
        fastcgi_pass host.docker.internal:9073;
        fastcgi_split_path_info ^(.+\.php)(/.*)$;
        include fastcgi_params;
        fastcgi_param PATH_INFO $fastcgi_path_info;
        fastcgi_param SCRIPT_FILENAME /Users/runner/Code$fastcgi_script_name;
    }
}
